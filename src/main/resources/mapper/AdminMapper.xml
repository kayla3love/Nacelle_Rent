<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.manager.nacelle_rent.dao.AdminMapper">
    <resultMap id="adminResultMap" type="com.manager.nacelle_rent.entity.Admin">
        <result property="adminId" column="admin_id" />
        <result property="adminPassword" column="admin_password" />
    </resultMap>

    <select id="getAdminById" parameterType="String" resultMap="adminResultMap">
        select admin_password
        from admin_info
        WHERE  admin_id= #{adminId}
    </select>
</mapper>